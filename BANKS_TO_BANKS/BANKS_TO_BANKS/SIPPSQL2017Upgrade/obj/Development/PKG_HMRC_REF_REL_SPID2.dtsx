                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/* Logic to set the Financial Year for Reporting */

DECLARE @FY_Start	VARCHAR(10)
DECLARE @FY_End		VARCHAR(10)
DECLARE @Month 		TINYINT
DECLARE @Current_Date	DATETIME

SET @Current_Date = GETDATE()
SET @Month = DATEPART(M,@Current_Date)

IF @Month &gt;= 1 AND @Month &lt;= 3
BEGIN
	SET @FY_Start = CONVERT(VARCHAR,YEAR(@Current_Date) - 2) + '-04-06'
	SET @FY_End = CONVERT(VARCHAR,YEAR(@Current_Date) - 1) + '-04-05'
END
ELSE
BEGIN
	IF @Month &gt;= 5 AND @Month &lt;= 12
	BEGIN
		SET @FY_Start = CONVERT(VARCHAR,YEAR(@Current_Date) - 1) + '-04-06'
		SET @FY_End = CONVERT(VARCHAR,YEAR(@Current_Date)) + '-04-05'
	END
	ELSE
	BEGIN
		IF @Month = 4
		BEGIN
			IF DATEPART(D,@Current_Date) &gt; 5
			BEGIN
				SET @FY_Start = CONVERT(VARCHAR,YEAR(@Current_Date) - 1) + '-04-06'
				SET @FY_End = CONVERT(VARCHAR,YEAR(@Current_Date)) + '-04-05'
			END
			ELSE
			BEGIN
				SET @FY_Start = CONVERT(VARCHAR,YEAR(@Current_Date) - 2) + '-04-06'
				SET @FY_End = CONVERT(VARCHAR,YEAR(@Current_Date) - 1) + '-04-05'
			END
		END
	END
END


/* Get all Bank Transactions for the Members being reported */
-- Get the bank_transactions from member account for single member from the following query

SELECT 
	bt.Bank_Transaction_ID,
	bt.movement_group_id, 
	bt.Investment_ID,
	bt.Settlement_Date,
	bt.Transaction_Type,
	bt.Transaction_Code,
	bt.Transaction_Amount , tcp.tenant_change_payment_id, tc.tenant_charge_id, tc.lease_id
FROM	Bank_Transactions BT
JOIN movement_groups mg ON mg.movement_group_id =  bt.movement_group_id 
LEFT OUTER JOIN tenant_charge_payments tcp ON (tcp.movement_group_id = bt.movement_group_id  AND tcp.movement_id = bt.movement_id )
LEFT OUTER JOIN tenant_charges tc ON tc.tenant_charge_id = tcp.tenant_charge_id 
WHERE	(Settlement_Date &gt;= @FY_Start AND
	 Settlement_Date &lt;= @FY_End)
AND	Transaction_Code IN ('2802','2803','2813')
AND	Investment_ID IN
	(SELECT Investment_ID FROM Investments WHERE ID IN 
		(SELECT investments.ID FROM Investments, owner WHERE Investment_ID IN 
			(SELECT Investment_ID FROM Purchase_Details WHERE Investment_ID IN
				(SELECT Investment_ID FROM Sale_Details WHERE
				 Completion IS NULL OR (Completion &gt;= @FY_Start  ))
			AND (Completion &lt;=  @FY_End  AND Completion IS NOT NULL AND Completion &lt;&gt; '1899-12-30')
			)
		AND owner.id = investments.id
		AND owner.owner_type = 'M' --Look only for members
		)
	 AND Investment_Type = 'B'
	)
AND (mg.writtenback IS NULL OR LTRIM(mg.writtenback) = '' OR UPPER(LTRIM(RTRIM(mg.writtenback)))='UPT')</property>
