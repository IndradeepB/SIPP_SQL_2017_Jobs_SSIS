                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/* Logic to set the Financial Year for Reporting */

DECLARE @FY_Start	VARCHAR(10)
DECLARE @FY_End		VARCHAR(10)
DECLARE @Month 		TINYINT
DECLARE @Current_Date	DATETIME

SET @Current_Date = GETDATE()
SET @Month = DATEPART(M,@Current_Date)

IF @Month &gt;= 1 AND @Month &lt;= 3
BEGIN
	SET @FY_Start = CONVERT(VARCHAR,YEAR(@Current_Date) - 2) + '-04-06'
	SET @FY_End = CONVERT(VARCHAR,YEAR(@Current_Date) - 1) + '-04-05'
END
ELSE
BEGIN
	IF @Month &gt;= 5 AND @Month &lt;= 12
	BEGIN
		SET @FY_Start = CONVERT(VARCHAR,YEAR(@Current_Date) - 1) + '-04-06'
		SET @FY_End = CONVERT(VARCHAR,YEAR(@Current_Date)) + '-04-05'
	END
	ELSE
	BEGIN
		IF @Month = 4
		BEGIN
			IF DATEPART(D,@Current_Date) &gt; 5
			BEGIN
				SET @FY_Start = CONVERT(VARCHAR,YEAR(@Current_Date) - 1) + '-04-06'
				SET @FY_End = CONVERT(VARCHAR,YEAR(@Current_Date)) + '-04-05'
			END
			ELSE
			BEGIN
				SET @FY_Start = CONVERT(VARCHAR,YEAR(@Current_Date) - 2) + '-04-06'
				SET @FY_End = CONVERT(VARCHAR,YEAR(@Current_Date) - 1) + '-04-05'
			END
		END
	END
END

/* Get all Investment and Member or Group Id's against them */

/* Get all Investment and Member or Group Id's against them */

SELECT
	DISTINCT(Investment_ID),
	ID,
	Investment_Type,
	Valuation,
	Valuation_Date,
	Sold,
	From_Connected_Party,
	To_Connected_Party
FROM	Investments
WHERE	
	Investment_ID IN
		(SELECT Investment_ID FROM Purchase_Details WHERE Investment_ID IN
			(SELECT Investment_ID FROM Sale_Details WHERE
			 Completion IS NULL OR (Completion &gt;= @FY_Start ))
		AND (Completion &lt;=  @FY_End AND Completion IS NOT NULL AND Completion &lt;&gt; '1899-12-30')
		)
OR	Investment_ID IN
	(SELECT Investment_ID FROM Investments WHERE ID IN 
		(SELECT ID FROM Investments WHERE Investment_ID IN 
			(SELECT Investment_ID FROM Purchase_Details WHERE Investment_ID IN
				(SELECT Investment_ID FROM Sale_Details WHERE
				 Completion IS NULL OR (Completion &gt;= @FY_Start ))
			AND (Completion &lt;=  @FY_End AND Completion IS NOT NULL AND Completion &lt;&gt; '1899-12-30')
			)
		)
	 AND Investment_Type = 'B'
	)</property>
