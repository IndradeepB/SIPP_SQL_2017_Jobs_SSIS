    <DTS:Executable
      DTS:refId="Package\Set_Financial_year"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Set_Financial_year"
      DTS:DTSID="{7744A0B4-711F-4608-8D57-B91A71167E74}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Set_Financial_year"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2;  2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{508DBEB3-0701-4C73-958A-AFDF68614E43}"
          SQLTask:SqlStatementSource="/* Logic to set the Financial Year for Reporting */&#xA;&#xA;DECLARE @FY_Start&#x9;VARCHAR(10)&#xA;DECLARE @FY_End&#x9;&#x9;VARCHAR(10)&#xA;DECLARE @Month &#x9;&#x9;TINYINT&#xA;DECLARE @Current_Date&#x9;DATETIME&#xA;&#xA;SET @Current_Date = GETDATE()&#xA;SET @Month = DATEPART(M,@Current_Date)&#xA;&#xA;IF @Month &gt;= 1 AND @Month &lt;= 3&#xA;BEGIN&#xA;&#x9;SET @FY_Start = CONVERT(VARCHAR,YEAR(@Current_Date) - 2) + '-04-06'&#xA;&#x9;SET @FY_End = CONVERT(VARCHAR,YEAR(@Current_Date) - 1) + '-04-05'&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;&#x9;IF @Month &gt;= 5 AND @Month &lt;= 12&#xA;&#x9;BEGIN&#xA;&#x9;&#x9;SET @FY_Start = CONVERT(VARCHAR,YEAR(@Current_Date) - 1) + '-04-06'&#xA;&#x9;&#x9;SET @FY_End = CONVERT(VARCHAR,YEAR(@Current_Date)) + '-04-05'&#xA;&#x9;END&#xA;&#x9;ELSE&#xA;&#x9;BEGIN&#xA;&#x9;&#x9;IF @Month = 4&#xA;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;IF DATEPART(D,@Current_Date) &gt; 5&#xA;&#x9;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;&#x9;SET @FY_Start = CONVERT(VARCHAR,YEAR(@Current_Date) - 1) + '-04-06'&#xA;&#x9;&#x9;&#x9;&#x9;SET @FY_End = CONVERT(VARCHAR,YEAR(@Current_Date)) + '-04-05'&#xA;&#x9;&#x9;&#x9;END&#xA;&#x9;&#x9;&#x9;ELSE&#xA;&#x9;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;&#x9;SET @FY_Start = CONVERT(VARCHAR,YEAR(@Current_Date) - 2) + '-04-06'&#xA;&#x9;&#x9;&#x9;&#x9;SET @FY_End = CONVERT(VARCHAR,YEAR(@Current_Date) - 1) + '-04-05'&#xA;&#x9;&#x9;&#x9;END&#xA;&#x9;&#x9;END&#xA;&#x9;END&#xA;END&#xA;&#xA;/* Update The HMRC Parameter Table. Here onwards everywhere the &#xA;Financial Start and End will be picked from Table. */&#xA;&#xA;UPDATE HMRC_Parameters SET Parameter_value = @FY_Start WHERE Parameter_Name = 'FY_Start'&#xA;UPDATE HMRC_Parameters SET Parameter_value = @FY_End WHERE Parameter_Name = 'FY_End'" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
